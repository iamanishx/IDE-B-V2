{
    "compilerOptions": {
      "module": "commonjs",
      "target": "ES2022",
      "strict": false,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "outDir": "./dist",
      "rootDir": "./src",
      "baseUrl": "./",
      "resolveJsonModule": true,
      "experimentalDecorators": true,
      "emitDecoratorMetadata": true
    },
    "include": ["src/**/*.ts"],
    "exclude": ["node_modules"]
  }
  